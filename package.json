{
  "name": "@codex-protocol/service.codex-title-registry-api",
  "version": "0.1.0",
  "description": "API for Codex Title Registry",
  "repository": {
    "type": "git",
    "url": "https://github.com/codex-protocol/service.codex-title-registry-api"
  },
  "contributors": [
    {
      "name": "John Forrest"
    },
    {
      "name": "Colin Wood"
    }
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/.",
    "watch": "nodemon -x npm start",
    "build": "npm run lint && npm run compile",
    "start": "npm run lint && babel-node src/index.js",
    "start:compiled": "npm run compile && node dist/index.js",
    "drop-db": "mongo codex-title-registry --eval 'db.dropDatabase()'",
    "compile": "npm run clean && babel src --out-dir dist"
  },
  "dependencies": {
    "agenda": "^1.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bignumber.js": "^6.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "ethereum-bip44": "^2.1.3",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-util": "^5.1.4",
    "express": "^4.16.2",
    "filewalker": "^0.1.3",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "lusca": "^1.5.2",
    "mongoose": "^4.13.11",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "restify-errors": "^6.0.0",
    "web3": "^1.0.0-beta.30",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0"
  }
}
